<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="ContactPopover" parent="GtkPopover">
    <property name="position">top</property>
    <property name="child">
      <object class="GtkGrid">
        <property name="halign">start</property>
        <property name="column-spacing">12</property>
        <child>
          <object class="GtkLabel" id="_jid">
            <property name="halign">start</property>
            <property name="valign">start</property>
            <property name="label">&lt;jid&gt;</property>
            <property name="ellipsize">end</property>
            <property name="max-width-chars">36</property>
            <property name="xalign">0</property>
            <layout>
              <property name="column">1</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="_tune_label">
            <property name="halign">end</property>
            <property name="valign">start</property>
            <property name="visible">false</property>
            <property name="label" translatable="yes">Tune:</property>
            <layout>
              <property name="column">0</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="_location_label">
            <property name="halign">end</property>
            <property name="valign">start</property>
            <property name="visible">false</property>
            <property name="label" translatable="yes">Location:</property>
            <layout>
              <property name="column">0</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="_tune">
            <property name="halign">start</property>
            <property name="valign">start</property>
            <property name="visible">false</property>
            <property name="hexpand">1</property>
            <layout>
              <property name="column">1</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="_location">
            <property name="halign">start</property>
            <property name="valign">start</property>
            <property name="visible">false</property>
            <property name="hexpand">1</property>
            <layout>
              <property name="column">1</property>
              <property name="row">4</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="_name">
            <property name="halign">start</property>
            <property name="valign">start</property>
            <property name="label">&lt;name&gt;</property>
            <property name="ellipsize">end</property>
            <property name="single-line-mode">1</property>
            <property name="max-width-chars">25</property>
            <property name="yalign">0</property>
            <style>
              <class name="title-1"/>
            </style>
            <layout>
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkImage" id="_avatar">
            <property name="halign">start</property>
            <property name="valign">start</property>
            <property name="icon-name">image-missing</property>
            <layout>
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="row-span">3</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="_sub_label">
            <property name="halign">end</property>
            <property name="valign">start</property>
            <property name="visible">false</property>
            <property name="label" translatable="yes">Subscription:</property>
            <layout>
              <property name="column">0</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="_sub">
            <property name="halign">start</property>
            <property name="valign">start</property>
            <property name="visible">false</property>
            <property name="hexpand">1</property>
            <layout>
              <property name="column">1</property>
              <property name="row">5</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="_resources_box">
            <property name="halign">start</property>
            <property name="valign">start</property>
            <property name="visible">false</property>
            <property name="orientation">vertical</property>
            <child>
              <placeholder/>
            </child>
            <layout>
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <style>
          <class name="p-3"/>
        </style>
      </object>
    </property>
  </template>
</interface>
