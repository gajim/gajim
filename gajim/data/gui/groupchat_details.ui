<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkStack" id="main_stack">
    <property name="vexpand">1</property>
    <property name="transition-type">crossfade</property>
    <property name="width-request">650</property>
    <style>
      <class name="mt-12"/>
    </style>
    <child>
      <object class="GtkStackPage">
        <property name="name">information</property>
        <property name="child">
          <object class="GtkBox" id="info_container">
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">settings</property>
        <property name="child">
          <object class="GtkScrolledWindow">
            <property name="focusable">1</property>
            <property name="hscrollbar-policy">never</property>
            <property name="child">
              <object class="GtkViewport">
                <property name="child">
                  <object class="AdwClamp">
                    <child>
                      <object class="GtkBox" id="settings_box">
                        <property name="orientation">vertical</property>
                        <property name="spacing">24</property>
                        <style>
                          <class name="py-3"/>
                        </style>
                        <child>
                          <placeholder/>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
              </object>
            </property>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">encryption-omemo</property>
        <property name="child">
          <object class="GtkScrolledWindow" id="encryption_scrolled">
            <property name="focusable">1</property>
            <property name="hscrollbar-policy">never</property>
            <property name="child">
              <object class="GtkViewport">
                <property name="child">
                  <object class="AdwClamp" id="encryption_box">
                  </object>
                </property>
              </object>
            </property>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">blocks</property>
        <property name="child">
          <object class="GtkBox" id="blocks_box">
            <property name="orientation">vertical</property>
            <child>
              <placeholder/>
            </child>
            <style>
              <class name="p-18"/>
            </style>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">manage</property>
        <property name="child">
          <object class="GtkBox" id="manage_box">
            <property name="orientation">vertical</property>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">affiliations</property>
        <property name="child">
          <object class="GtkBox" id="affiliation_box">
            <property name="orientation">vertical</property>
            <child>
              <placeholder/>
            </child>
            <style>
              <class name="p-18"/>
            </style>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">outcasts</property>
        <property name="child">
          <object class="GtkBox" id="outcasts_box">
            <property name="orientation">vertical</property>
            <child>
              <placeholder/>
            </child>
            <style>
              <class name="p-18"/>
            </style>
          </object>
        </property>
      </object>
    </child>
    <child>
      <object class="GtkStackPage">
        <property name="name">config</property>
        <property name="child">
          <object class="GtkBox" id="configuration_box">
            <property name="orientation">vertical</property>
            <child>
              <placeholder/>
            </child>
          </object>
        </property>
      </object>
    </child>
  </object>
</interface>
